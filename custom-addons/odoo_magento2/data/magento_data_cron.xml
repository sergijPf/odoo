<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_automatic_workflow_job" model="ir.cron">
            <field name="name">Magento Orders Auto Workflow Job</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" eval="ref('odoo_magento2.model_sale_workflow_process')"/>
            <field name="state">code</field>
            <field name="code">model.auto_workflow_process()</field>
        </record>

        <record forcecreate="True" id="ir_cron_update_products_export_statuses" model="ir.cron">
            <field name="name"> Magento - Update Products Export Statuses </field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model_id" eval="ref('odoo_magento2.model_magento_configurable_product')"/>
            <field name="state">code</field>
            <field name="code">model.processing_products_export_to_magento(True, True)</field>
        </record>
    </data>
</odoo>
